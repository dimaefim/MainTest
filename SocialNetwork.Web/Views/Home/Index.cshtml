@model SocialNetwork.Models.Models.MainPageViewModel

@{
    Layout = "../Shared/_Layout.cshtml";
}
<div class="row">

    <!--Фото-->
    <div class="col-md-4">
        <img src="~/Content/Home/nophoto.jpg" class="main-photo img-responsive">

        <ul class="nav nav-pills">
            <li class="">@Html.ActionLink("Редактировать фото", "Index", "Home")</li>
        </ul>
    </div>

    <!--Описание-->
    <div class="col-md-8">
        <h3>@Model.Surname @Model.Name</h3>
        <table class="table">
            <tbody>
            <tr>
                <td>Дата Рождения</td>
                <td>@Model.DateOfBirth</td>
            </tr>
            </tbody>
        </table>
        <ul class="nav nav-pills">
            <li class="">@Html.ActionLink("Редактировать личную информацию", "EditUserData", "Home")</li>
        </ul>
    </div>

</div>